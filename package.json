{
  "name": "translatty",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev:start": "npm-run-all --parallel dev:client:build:watch dev:server:build:watch dev:server:start:watch css:watch",
    "dev:client:build:watch": "webpack -w -c ./webpack/client.config.js",
    "dev:server:build:watch": "webpack -w -c ./webpack/server.config.js",
    "dev:server:start:watch": "supervisor ./build/server/main.js",
    "dev:analize-bundle": "ANALYZE_BUNDLE=1 npm run dev:client:build:watch",
    "prod:start": "NODE_ENV=production npm-run-all --sequential prod:client:build prod:server:build prod:server:start",
    "prod:client:build": "NODE_ENV=production webpack -c ./webpack/client.config.js",
    "prod:server:build": "NODE_ENV=production webpack -c ./webpack/server.config.js",
    "prod:server:start": "node ./build/server/main.js",
    "prod:analize-bundle": "ANALYZE_BUNDLE=1 npm run prod:client:build",
    "css:watch": "tcm -p app/**/*.pcss --watch",
    "db:generate": "ts-node -r tsconfig-paths/register app/server/scripts/generateDB.ts",
    "db:migrate": "ts-node -r tsconfig-paths/register app/server/scripts/migrateDB.ts",
    "parse-subs": "ts-node -r tsconfig-paths/register app/server/scripts/parseSubs.ts",
    "typecheck": "tsc --noEmit",
    "eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "eslint-fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix"
  },
  "alias": {
    "server": "./app/server",
    "client": "./app/client",
    "common": "./app/common"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boarder-portal/translatty.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/boarder-portal/translatty/issues"
  },
  "homepage": "https://github.com/boarder-portal/translatty#readme",
  "dependencies": {
    "@loadable/component": "^5.15.0",
    "@loadable/server": "^5.15.0",
    "@types/connect-redis": "0.0.14",
    "@types/redis": "^2.8.25",
    "boarder-components": "^1.3.8",
    "classnames": "^2.3.1",
    "connect-redis": "^5.0.0",
    "dayjs": "^1.10.4",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "fast-deep-equal": "^3.1.3",
    "fs-extra": "^9.0.0",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.1.1",
    "recoil": "^0.5.2",
    "redis": "^3.0.2",
    "regenerator-runtime": "^0.13.7",
    "ts-node": "^8.6.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^4.5.3",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@loadable/babel-plugin": "^5.13.2",
    "@loadable/webpack-plugin": "^5.15.0",
    "@types/express": "^4.17.2",
    "@types/express-session": "^1.17.0",
    "@types/fs-extra": "^9.0.0",
    "@types/loadable__component": "^5.13.3",
    "@types/loadable__server": "^5.12.5",
    "@types/lodash": "^4.14.178",
    "@types/morgan": "^1.7.37",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.2",
    "@types/uuid": "^3.4.7",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.3",
    "body-parser": "^1.19.0",
    "css-loader": "^5.2.6",
    "csstype": "^3.0.8",
    "eslint": "^8.4.0",
    "eslint-plugin-import-helpers": "1.0.2",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^1.6.0",
    "morgan": "^1.9.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.4",
    "postcss-loader": "^6.2.1",
    "postcss-nested": "^5.0.6",
    "prettier": "^2.5.1",
    "supervisor": "^0.12.0",
    "tsconfig-paths": "^3.9.0",
    "typed-css-modules": "^0.7.0",
    "webpack": "^5.38.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.0"
  }
}
